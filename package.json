{
  "name": "metavm",
  "version": "1.2.1",
  "author": "Timur Shemsedinov <timur.shemsedinov@gmail.com>",
  "license": "MIT",
  "description": "Metarhia script loader, node.js vm wrapper",
  "keywords": [
    "node.js",
    "metarhia",
    "vm",
    "sandbox",
    "context",
    "script",
    "container",
    "isolation"
  ],
  "main": "metavm.js",
  "module": "./dist/metavm.mjs",
  "types": "metavm.d.ts",
  "browser": {
    "./metavm.js": "./dist/metavm.js"
  },
  "files": [
    "dist/",
    "metavm.d.ts"
  ],
  "engines": {
    "node": "^14.6 || 16 || 18"
  },
  "exports": {
    "import": "./dist/metavm.mjs",
    "require": "./metavm.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/metarhia/metavm.git"
  },
  "bugs": {
    "url": "https://github.com/metarhia/metavm/issues",
    "email": "timur.shemsedinov@gmail.com"
  },
  "homepage": "https://metarhia.com",
  "funding": {
    "type": "patreon",
    "url": "https://www.patreon.com/tshemsedinov"
  },
  "scripts": {
    "lint": "eslint . && prettier -c \"**/*.js\" \"**/*.json\" \"**/*.md\" \"**/.*rc\" \"**/*.ts\"",
    "types": "tsc -p tsconfig.json",
    "fmt": "prettier --write \"**/*.js\" \"**/*.json\" \"**/*.md\" \"**/.*rc\" \"**/*.ts\"",
    "test:browser": "metatests test/browser/unit.js",
    "test:node": "metatests test/unit.js && metatests test/timeout.js",
    "test": "npm run lint && npm run types && npm run test:node && npm run test:browser"
  },
  "devDependencies": {
    "@types/node": "^18.0.3",
    "eslint": "^8.19.0",
    "eslint-config-metarhia": "^8.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "metatests": "^0.8.2",
    "prettier": "^2.7.1",
    "typescript": "^4.7.4",
    "webdriverio": "^7.21.0"
  }
}
