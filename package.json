{
  "name": "metavm",
  "version": "0.0.1",
  "description": "Metarhia script loader, node.js vm wrapper",
  "main": "metavm.js",
  "files": [],
  "scripts": {
    "lint": "eslint . && prettier --check \"**/*.js\" \"**/*.json\" \"**/*.md\" \"**/.*rc\" \"**/*.yml\" \"**/*.ts\"",
    "fmt": "prettier --write \"**/*.js\" \"**/*.json\" \"**/*.md\" \"**/.*rc\" \"**/*.yml\" \"**/*.ts\"",
    "types": "tsc -p types/tsconfig.json",
    "test": "eslint . && metatests test/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/metarhia/metavm.git"
  },
  "keywords": [
    "node.js",
    "metarhia",
    "vm"
  ],
  "author": "Timur Shemsedinov <timur.shemsedinov@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/metarhia/metavm/issues"
  },
  "homepage": "https://github.com/metarhia/metavm#readme",
  "devDependencies": {
    "eslint": "^7.8.1",
    "eslint-config-metarhia": "^7.0.1",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-prettier": "^3.1.4",
    "prettier": "^1.19.1",
    "typescript": "^4.0.2",
    "metatests": "^0.7.2"
  }
}
