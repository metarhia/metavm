{
  "name": "metavm",
  "version": "1.4.1",
  "author": "Timur Shemsedinov <timur.shemsedinov@gmail.com>",
  "license": "MIT",
  "description": "Metarhia script loader, node.js vm wrapper",
  "keywords": [
    "node.js",
    "metarhia",
    "vm",
    "sandbox",
    "context",
    "script",
    "container",
    "isolation"
  ],
  "main": "metavm.js",
  "types": "metavm.d.ts",
  "readmeFilename": "README.md",
  "browser": {
    "./metavm.js": "./dist/metavm.js"
  },
  "files": [
    "dist/",
    "metavm.d.ts"
  ],
  "engines": {
    "node": "18 || 20 || 21"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/metarhia/metavm.git"
  },
  "bugs": {
    "url": "https://github.com/metarhia/metavm/issues",
    "email": "timur.shemsedinov@gmail.com"
  },
  "homepage": "https://metarhia.com",
  "funding": {
    "type": "patreon",
    "url": "https://www.patreon.com/tshemsedinov"
  },
  "scripts": {
    "lint": "eslint . && prettier -c \"**/*.js\" \"**/*.json\" \"**/*.md\" \"**/.*rc\" \"**/*.ts\"",
    "types": "tsc -p tsconfig.json",
    "fmt": "prettier --write \"**/*.js\" \"**/*.json\" \"**/*.md\" \"**/.*rc\" \"**/*.ts\"",
    "test": "npm run lint && npm run types && node --test"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "eslint": "^8.55.0",
    "eslint-config-metarhia": "^8.2.2",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-prettier": "^5.0.0",
    "prettier": "^3.1.1",
    "typescript": "^5.3.3"
  }
}
